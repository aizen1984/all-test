<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!--导入属性配置 -->
    <!--<properties url="file:////Users/roderickWang/git/caijiajia/arenamgr/mybatisGenerator/generator.properties"></properties>-->
    <!--<properties url="file:////Users/zlh/cjj/arenamgr/mybatisGenerator/generator.properties"></properties>-->

    <!--指定特定数据库的jdbc驱动jar包的位置 -->
    <classPathEntry location="mysql-connector-java-5.1.37-bin.jar"/>

    <context id="default" targetRuntime="MyBatis3">

        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <!-- optional，旨在创建class时，对注释进行控制 -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
        </commentGenerator>


        <!--jdbc的数据库连接 -->
        <!--<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.32.206:3306/loan"-->
        <!--userId="root" password="root">-->
        <!--</jdbcConnection>-->
        <!--<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/loan"-->
        <!--userId="root" password="">-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/all-test?characterEncoding=utf8"
                        userId="root" password="root">
        </jdbcConnection>


        <!-- 非必需，类型处理器，在数据库类型和java类型之间的转换控制-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- Model模型生成器,用来生成含有主键key的类，记录类 以及查询Example类
            targetPackage     指定生成的model生成所在的包名
            targetProject     指定在该项目下所在的路径
        -->
        <javaModelGenerator targetPackage="com.ethan.test.domain" targetProject="../src/main/java">
            <!-- 是否对model添加 构造函数 -->
            <property name="constructorBased" value="true"/>

            <!-- 是否允许子包，即targetPackage.schemaName.tableName -->
            <property name="enableSubPackages" value="false"/>

            <!-- 建立的Model对象是否 不可改变  即生成的Model对象不会有 setter方法，只有构造方法 -->
            <property name="immutable" value="false"/>

            <!-- 给Model添加一个父类 -->

            <!-- 是否对类CHAR类型的列的数据进行trim操作 -->
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!--Mapper映射文件生成所在的目录 为每一个数据库的表生成对应的SqlMap文件 -->
        <sqlMapGenerator targetPackage="com.ethan.test.mapper" targetProject="../src/main/resources">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>


        <!-- 客户端代码，生成易于使用的针对Model对象和XML配置文件 的代码
                type="ANNOTATEDMAPPER",生成Java Model 和基于注解的Mapper对象
                type="MIXEDMAPPER",生成基于注解的Java Model 和相应的Mapper对象
                type="XMLMAPPER",生成SQLMap XML文件和独立的Mapper接口
        -->
        <javaClientGenerator targetPackage="com.ethan.test.mapper" targetProject="../src/main/java"
                             type="MIXEDMAPPER">
            <property name="enableSubPackages" value=""/>
            <!--
                    定义Maper.java 源代码中的ByExample() 方法的可视性，可选的值有：
                    public;
                    private;
                    protected;
                    default
                    注意：如果 targetRuntime="MyBatis3",此参数被忽略
             -->
            <property name="exampleMethodVisibility" value=""/>
            <!--
              方法名计数器
              Important note: this property is ignored if the target runtime is MyBatis3.
             -->
            <property name="methodNameCalculator" value=""/>

            <!--为生成的接口添加父接口-->
            <property name="rootInterface" value=""/>

        </javaClientGenerator>
        <!--<table tableName="bank_pipeline_process" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="t_lend_trans" delimitIdentifiers="true"  delimitAllColumns="true"></table>-->
        <table tableName="t_apply_info" delimitIdentifiers="true" delimitAllColumns="true"></table>
        <!--<table tableName="channel_card" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="credit_info" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="deduct_detail" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="trans_log" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="stage_order" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="channel_contract" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="stage_order" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="stage_plan" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
        <!--<table tableName="trans_log" delimitIdentifiers="true" delimitAllColumns="true"></table>-->
    </context>
</generatorConfiguration>
